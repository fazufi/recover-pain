{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "plugins": ["prettier", "react"],
  "rules": {
    "@next/next/no-img-element": "off",
    "@next/next/no-page-custom-font": "off",
    "@next/next/google-font-display": "off",
